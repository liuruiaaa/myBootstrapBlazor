            <div class="panel">Panel1</div>
        </DockViewComponent>
        <DockViewComponent Title="标签二">
            <TitleTemplate>
                <DockViewTitleTemplateDemo OnClickTitleBarCallback="OnClickTitleBarCallback"></DockViewTitleTemplateDemo>
